{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-g",
                "-Wall",
                "-Werror",
                "-Wextra",
                "${workspaceFolder}/*.h",
                "${workspaceFolder}/libft/*.h",
                "${workspaceFolder}/libft/*.c",
                "${workspaceFolder}/src/*.c",
                "${workspaceFolder}/get_next_line/*.c",
                "${workspaceFolder}/get_next_line/*.h"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}